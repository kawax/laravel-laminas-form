{
  "name": "revolution/laravel-laminas-form",
  "description": "Laravel Laminas Form",
  "keywords": [
    "laminas-form",
    "form",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "laminas/laminas-escaper": "^2.12",
    "laminas/laminas-form": "^3.8",
    "laminas/laminas-i18n": "^2.21",
    "laminas/laminas-servicemanager": "^3.20",
    "laminas/laminas-view": "^2.26",
    "illuminate/support": "^11.0||^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0||^10.0"
  },
  "autoload": {
    "psr-4": {
      "Revolution\\LaminasForm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Revolution\\LaminasForm\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "kawax",
      "email": "kawaxbiz@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Revolution\\LaminasForm\\Providers\\LaminasFormServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
