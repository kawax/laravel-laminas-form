{
  "name": "revolution/laravel-laminas-form",
  "description": "Laravel Laminas Form",
  "keywords": [
    "laminas-form",
    "form",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3||^8.0",
    "laminas/laminas-escaper": "^2.6",
    "laminas/laminas-form": "^2.14||^3.0",
    "laminas/laminas-i18n": "^2.10",
    "laminas/laminas-servicemanager": "^3.4",
    "laminas/laminas-view": "^2.11",
    "illuminate/support": "^6.0||^7.0||^8.0",
    "illuminate/container": "^6.0||^7.0||^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0||^9.0",
    "mockery/mockery": "^1.3",
    "orchestra/testbench": "^4.0||^5.0||^6.0"
  },
  "autoload": {
    "psr-4": {
      "Revolution\\LaminasForm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Revolution\\LaminasForm\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "kawax",
      "email": "kawaxbiz@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Revolution\\LaminasForm\\Providers\\LaminasFormServiceProvider"
      ]
    }
  },
  "config": {
    "platform": {
      "php": "8.0.0"
    }
  }
}
